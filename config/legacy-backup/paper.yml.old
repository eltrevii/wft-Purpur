world-settings:
  default:
    slime-spawn-height:
      swamp-biome:
        maximum: 70.0
        minimum: 50.0
      slime-chunk:
        maximum: 40.0
    wateranimal-spawn-height:
      minimum: default
      maximum: default
    monster-spawn-max-light-level: -1
    anticheat:
      obfuscation:
        items:
          hide-itemmeta: false
          hide-durability: false
    disable-thunder: false
    disable-ice-and-snow: false
    keep-spawn-loaded-range: 10
    map-item-frame-cursor-limit: 128
    armor-stands-tick: true
    enable-treasure-maps: true
    treasure-maps-return-already-discovered: false
    split-overstacked-loot: true
    all-chunks-are-slime-chunks: false
    should-remove-dragon: true
    optimize-explosions: true
    experience-merge-max-value: -1
    piglins-guard-chests: true
    fixed-chunk-inhabited-time: -1
    remove-corrupt-tile-entities: false
    falling-block-height-nerf: 0
    tnt-entity-height-nerf: 0
    max-entity-collisions: 8
    phantoms-do-not-spawn-on-creative-players: true
    phantoms-only-attack-insomniacs: true
    fix-wither-targeting-bug: true
    allow-using-signs-inside-spawn-protection: true
    ender-dragons-death-always-places-dragon-egg: false
    non-player-arrow-despawn-rate: -1
    creative-arrow-despawn-rate: -1
    spawner-nerfed-mobs-should-jump: false
    entities-target-with-follow-range: false
    zombie-villager-infection-chance: -1.0
    zombies-target-turtle-eggs: false
    delay-chunk-unloads-by: 10s
    count-all-mobs-for-spawning: false
    prevent-moving-into-unloaded-chunks: true
    use-vanilla-world-scoreboard-name-coloring: false
    max-auto-save-chunks-per-tick: 24
    baby-zombie-movement-modifier: 0.5
    duplicate-uuid-resolver: saferegen
    duplicate-uuid-saferegen-delete-range: 32
    disable-creeper-lingering-effect: true
    filter-nbt-data-from-spawn-eggs-and-related: true
    allow-player-cramming-damage: false
    map-item-frame-cursor-update-interval: 10
    update-pathfinding-on-block-update: true
    light-queue-size: 20
    auto-save-interval: -1
    container-update-tick-rate: 1
    iron-golems-can-spawn-in-air: false
    mob-spawner-tick-rate: 1
    only-players-collide: false
    allow-vehicle-collisions: true
    portal-search-radius: 128
    portal-create-radius: 16
    portal-search-vanilla-dimension-scaling: true
    water-over-lava-flow-speed: 5
    keep-spawn-loaded: false
    disable-teleportation-suffocation-check: false
    show-sign-click-command-failure-msgs-to-player: false
    use-faster-eigencraft-redstone: true
    prevent-tnt-from-moving-in-water: false
    per-player-mob-spawns: true
    seed-based-feature-search: true
    seed-based-feature-search-loads-chunks: true
    nether-ceiling-void-damage-height: 0
    allow-non-player-entities-on-scoreboards: false
    parrots-are-unaffected-by-player-movement: false
    disable-explosion-knockback: false
    fix-items-merging-through-walls: true
    fix-climbing-bypassing-cramming-rule: true
    armor-stands-do-collision-entity-lookups: true
    skeleton-horse-thunder-spawn-chance: -1
    max-leash-distance: 10.0
    grass-spread-tick-rate: 1
    viewdistances:
      no-tick-view-distance: -1
    game-mechanics:
      disable-end-credits: true
      disable-mob-spawner-spawn-egg-transformation: true
      fix-curing-zombie-villager-discount-exploit: true
      disable-pillager-patrols: false
      scan-for-legacy-ender-dragon: false
      disable-relative-projectile-velocity: false
      disable-unloaded-chunk-enderpearl-exploit: true
      disable-player-crits: false
      disable-chest-cat-detection: false
      nerf-pigmen-from-nether-portals: false
      disable-sprint-interruption-on-attack: false
      shield-blocking-delay: 5
      pillager-patrols:
        spawn-chance: 0.2
        spawn-delay:
          per-player: false
          ticks: 12000
        start:
          per-player: false
          day: 5
    generator-settings:
      flat-bedrock: true
    squid-spawn-height:
      maximum: 0.0
    fishing-time-range:
      MinimumTicks: 100
      MaximumTicks: 600
    max-growth-height:
      cactus: 3
      reeds: 3
      bamboo:
        max: 16
        min: 11
    despawn-ranges:
      axolotls:
        soft: 32
        hard: 128
      monster:
        soft: 32
        hard: 128
      creature:
        soft: 32
        hard: 128
      ambient:
        soft: 32
        hard: 128
      underground_water_creature:
        soft: 32
        hard: 128
      water_creature:
        soft: 32
        hard: 128
      water_ambient:
        soft: 32
        hard: 64
      misc:
        soft: 32
        hard: 128
    frosted-ice:
      enabled: false
      delay:
        min: 20
        max: 40
    lootables:
      auto-replenish: false
      restrict-player-reloot: true
      reset-seed-on-fill: true
      max-refills: -1
      refresh-min: 12h
      refresh-max: 2d
    mob-effects:
      undead-immune-to-certain-effects: true
      spiders-immune-to-poison-effect: true
      immune-to-wither-effect:
        wither: true
        wither-skeleton: true
    alt-item-despawn-rate:
      enabled: false
      items:
        COBBLESTONE: 300
    tick-rates:
      sensor:
        villager:
          secondarypoisensor: 40
      behavior:
        villager:
          validatenearbypoi: -1
    unsupported-settings:
      fix-invulnerable-end-crystal-exploit: true
    wandering-trader:
      spawn-minute-length: 1200
      spawn-day-length: 24000
      spawn-chance-failure-increment: 25
      spawn-chance-min: 25
      spawn-chance-max: 75
    spawn-limits:
      axolotls: -1
      monster: -1
      creature: -1
      ambient: -1
      underground_water_creature: -1
      water_creature: -1
      water_ambient: -1
    door-breaking-difficulty:
      zombie:
      - HARD
      vindicator:
      - NORMAL
      - HARD
    entity-per-chunk-save-limit:
      experience_orb: -1
      snowball: -1
      ender_pearl: -1
      arrow: -1
      fireball: -1
      small_fireball: -1
    mobs-can-always-pick-up-loot:
      zombies: false
      skeletons: false
    hopper:
      cooldown-when-full: false
      disable-move-event: true
      ignore-occluding-blocks: true
    feature-seeds:
      generate-random-seeds-for-all: false
    anti-xray:
      enabled: true
      engine-mode: 2
      max-block-height: 80
      update-radius: 2
      lava-obscures: false
      use-permission: false
      hidden-blocks:
      - copper_ore
      - deepslate_copper_ore
      - gold_ore
      - deepslate_gold_ore
      - iron_ore
      - deepslate_iron_ore
      - coal_ore
      - deepslate_coal_ore
      - lapis_ore
      - deepslate_lapis_ore
      - mossy_cobblestone
      - obsidian
      - chest
      - diamond_ore
      - deepslate_diamond_ore
      - redstone_ore
      - deepslate_redstone_ore
      - clay
      - emerald_ore
      - deepslate_emerald_ore
      - ender_chest
      replacement-blocks:
      - stone
      - oak_planks
      - deepslate
